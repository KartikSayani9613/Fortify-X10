for i -> 0#10 do
	if i<5 then
		println "I am small"
	else 
		println "I am large"
	end
end





for(i in 0..9){
	if(i<5){
		Console.OUT.println("I am small");
	}
	else{
		Console.OUT.println("I am large");	
	}
}
